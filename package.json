{
  "name": "prisma-tutorial",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "dotenv -- nodemon ./src/index.ts",
    "start": "dotenv -- node --experimental-transform-types ./src/index.ts",
    "test:unit": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "migration:create": "prisma migrate dev --create-only --skip-seed",
    "migration:apply": "prisma migrate deploy && prisma generate",
    "db:seed": "prisma db seed",
    "lint": "eslint"
  },
  "author": "Ale Scythecult Shov",
  "license": "ISC",
  "description": "Prisma tutorial",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@faker-js/faker": "^9.9.0",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.2.1",
    "@types/nodemailer": "^7.0.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.14.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "http-status-codes": "^2.3.0",
    "jose": "^6.0.12",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.0.17"
  }
}
